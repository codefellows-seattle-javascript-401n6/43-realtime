                   {this.state.messages.map(message ,inde => {\n                        return (\n                            <li>{this.state.username} : {this.state.message}</li>\n                        )\n                    })}\n                </ul>\n            </div>\n        </React.Fragment>\n    }\n}\n"},"lineCount":null}},"hash":"2690f4e3825ba95a0d5574a8d2d1cad7","cacheData":{"env":{}}}